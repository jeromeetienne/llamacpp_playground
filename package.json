{
  "name": "llama_playground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "run:testbed": "node ./examples/testbed-direct.js",
    "run:langchain": "node ./examples/basic-chat-langchain.js",
    "run:direct": "node ./examples/basic-chat-direct.js",
    "benchmark": "node ./examples/benchmark-direct.js  2>/dev/null",
    "generate:modelpaths": "node ./bin/generate_model_paths.js > ./src/model_path_constants.js",
    "download:llama-2-7b-chat": "cd models && wget -c https://huggingface.co/TheBloke/Llama-2-7b-Chat-GGUF/resolve/main/llama-2-7b-chat.Q6_K.gguf",
    "download:llama-2-13b-chat": "cd models && wget -c https://huggingface.co/TheBloke/Llama-2-13B-chat-GGUF/resolve/main/llama-2-13b-chat.Q3_K_M.gguf",
    "download:mistral-7b-instruct": "cd models && wget -c https://huggingface.co/TheBloke/Mistral-7B-Instruct-v0.1-GGUF/resolve/main/mistral-7b-instruct-v0.1.Q6_K.gguf",
    "download:zephyr-7b-alpha": "cd models && wget -c https://huggingface.co/TheBloke/zephyr-7B-alpha-GGUF/resolve/main/zephyr-7b-alpha.Q6_K.gguf",
    "download:zephyr-7b-beta": "cd models && wget -c https://huggingface.co/TheBloke/zephyr-7B-beta-GGUF/resolve/main/zephyr-7b-beta.Q6_K.gguf",
    "download:codellama-7b-instruct": "cd models && wget -c https://huggingface.co/TheBloke/CodeLlama-7B-Instruct-GGUF/resolve/main/codellama-7b-instruct.Q4_K_M.gguf",
    "download:codellama-13b-instruct": "cd models && wget -c https://huggingface.co/TheBloke/CodeLlama-13B-Instruct-GGUF/resolve/main/codellama-13b-instruct.Q3_K_M.gguf",
    "validate:llama-2-7b-chat": "npx node-llama-cpp chat --model ./models/llama-2-7b-chat.Q6_K.gguf",
    "validate:llama-2-13b-chat": "npx node-llama-cpp chat --model ./models/llama-2-13b-chat.Q3_K_M.gguf",
    "validate:mistral-7b-instruct": "npx node-llama-cpp chat --model ./models/mistral-7b-instruct-v0.1.Q6_K.gguf",
    "validate:zephyr-7b-alpha": "npx node-llama-cpp chat --model ./models/zephyr-7b-alpha.Q6_K.gguf",
    "validate:zephyr-7b-beta": "npx node-llama-cpp chat --model ./models/zephyr-7b-beta.Q6_K.gguf",
    "validate:codellama-7b-instruct": "npx node-llama-cpp chat --model ./models/codellama-7b-instruct.Q4_K_M.gguf",
    "validate:codellama-13b-instruct": "npx node-llama-cpp chat --model ./models/codellama-13b-instruct.Q3_K_M.gguf",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
